<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/multi_select_layout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_gravity="center"
    android:background="@drawable/contact_list_seperator"
    android:orientation="vertical"
    android:paddingBottom="2dp">

    <kore.botssdk.views.CustomTextViewBold
        android:id="@+id/tvform_template_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dimen_10dp"
        android:layout_marginTop="@dimen/dimen_10dp"
        android:text="Testing"
        android:textColor="#404051"
        android:textSize="@dimen/text_size_normal"
        android:textStyle="bold" />

    <kore.botssdk.view.AutoExpandListView
        android:id="@+id/multi_select_list"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:divider="@null"
        android:visibility="visible" />
</LinearLayout>
