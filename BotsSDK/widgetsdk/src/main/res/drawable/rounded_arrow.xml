<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval">
            <solid android:color="@color/color_7165250_widget" />
            <stroke
                android:width="1dp"
                android:color="@color/splash_background_color"/>

        </shape>
    </item>
    <item
        android:top="5dp"
        android:left="5dp"
        android:bottom="5dp"
        android:right="5dp"
